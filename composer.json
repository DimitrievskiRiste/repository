{
  "name": "riste/repository",
  "version": "1.0.0",
  "description": "Allow you to easily cache laravel models and retrieve from laravel's cache by extending AbstractRepository",
  "autoload": {
    "psr-4": {
      "Riste\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Riste\\Tests\\": "tests/"
    }
  },
  "require": {
    "laravel/laravel": ">=12",
    "php": ">=8.3"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/DimitrievskiRiste/repository"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "*"
  }
}